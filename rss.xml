---
layout: null
---

<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:dc="http://purl.org/dc/elements/1.1/">
  <channel>
    <title>{{ site.name }}</title>
    <description>{{ site.description }}</description>

    <image>
      <url>
        http://maciejczyzewski.me/assets/images/shortcuts/144.png
      </url>
      <title>{{ site.name }}</title>
      <link>{{ site.url }}</link>
    </image>

    <managingEditor>mail@maciejczyzewski.me (Maciej A. Czyzewski)</managingEditor>
    <copyright>Copyright Â© {{ site.time | date: "%Y" }} Maciej A. Czyzewski. All rights reserved.</copyright>

    <link>{{ site.url }}</link>
    <atom:link href="{{ site.url }}/rss.xml" rel="self" type="application/rss+xml" />

    <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

    {% for post in site.posts limit:10 %}
      <item>
        <title>{{ post.title | xml_escape }}</title>
        <description><![CDATA[ {{ post.content }} ]]></description>
        <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <link>{{ site.url }}{{ post.url }}?utm_source=Feed&amp;utm_medium=RSS</link>
        <guid isPermaLink="true">{{ site.url }}{{ post.url }}</guid>
      </item>
    {% endfor %}
  </channel>
</rss>
